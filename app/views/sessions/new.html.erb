<%= title t(:login) %>

<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :login %>
    <%= text_field_tag :login %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag t(:login), :class => :submit %>
<% end %>

<%= button_to t('helpers.submit.calendar'), bookings_path, {:method => :get, :class => :cancel} %>

<div class="clear"></div>
