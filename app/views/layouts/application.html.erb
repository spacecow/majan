<!DOCTYPE html>
<html>
<head>
  <title>Majan</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield(:css) %>
</head>
<body>

<div id="main_container">
  <div id="page">
    <% flash.each do |name,msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>
    <div class="clear"></div>
    <%= render 'layouts/user_nav' %>
    <%= yield %>
  </div>
</div>

</body>
</html>
